/* 字体图标 */

@font-face {
  font-family: 'iconfont';
  src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
         url(data:application/font-woff;charset=utf-8;base64,) format('woff');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-menu:before {
  content: "\e6ac";
}

.icon-cart:before {
  content: "\e603";
}

.icon-arrowleft:before {
  content: "\e602";
}

.icon-shoucang:before {
  content: "\e60c";
}

.icon-kefu:before {
  content: "\e88f";
}

.icon-shoucang1:before {
  content: "\e77f";
}

page {
  color: #252525;
  font-size: 12px;
}

/* 头部样式 */

.detile .header {
  overflow: hidden;
  background: #de4943;
}

.detile .header text:first-child, .detile .header text:last-child {
  float: left;
  width: 10%;
}

.detile .header text:nth-child(2) {
  float: left;
  width: 80%;
  font-size: 16px;
}

.detile .header text {
  line-height: 40px;
  text-align: center;
  color: #fff;
}

.detile .footer {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
}

.detile .footer .ul {
  display: flex;
  width: 188px;
}

.detile .footer .ul .li {
  flex: 1;
  height: 44px;
}

.detile .footer .ul .li text {
  display: block;
  text-align: center;
  line-height: 20px;
}

.detile .footer .btn {
  flex: 1;
  display: flex;
}

.detile .footer .btn text {
  flex: 1;
  line-height: 44px;
  text-align: center;
  font-size: 14px;
  color: #fff;
}

.detile .footer .btn text:first-child {
  background: #fc5a5a;
}

.detile .footer .btn text:last-child {
  background: #3c3f51;
}
